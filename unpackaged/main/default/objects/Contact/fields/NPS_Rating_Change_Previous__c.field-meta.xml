<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NPS_Rating_Change_Previous__c</fullName>
    <externalId>false</externalId>
    <formula>IF
(
    AND
    (
        NPS_Rating_Latest__c &lt;&gt; 0 ,
        NPS_Rating_Previous__c &lt;&gt; 0 
    )
    ,
    ( NPS_Rating_Latest__c - NPS_Rating_Previous__c )
    ,
    0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>NPS Rating Change - Previous</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
