<apex:page controller="WorkIt2.RelativeEditController" setup="true" title="Related Object Edit - WorkIt! Setup">
    <apex:form >    
        <apex:sectionHeader title="WorkIt! Setup" subtitle="Context Manager"/>
        <apex:pageBlock mode="edit" title="Related Object Edit">
            <apex:pageBlockButtons >
                <apex:commandButton value="Save" action="{!SaveReturn}"/>
                <apex:commandButton value="Cancel" action="{!CancelReturn}" immediate="true"/>                
            </apex:pageBlockButtons>

            <apex:pageBlockSection columns="1">
            <apex:inputField value="{!r.Name}"/>
            <apex:inputField value="{!r.WorkIt2__Rel_Field_Name__c}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
        
    </apex:form>
</apex:page>