    <script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/GSVendor/js/vendor/HTMLSanitizer.js"></script>
        <script>
            var hc= fakeEval('{!JSINHTMLENCODE(headerConfig)}');
            var apiEndPoint = hc.gatewayURL;
            var authorization= "";
			
            var appSessionId = hc.appSessionId;
            var appUserId = hc.appUserId;
            var appOrgId = hc.appOrgId;
            var GS = GS || {};
            GS.commonRemotingEndPoint   = '{!$RemoteAction.GSCommonBroker.gsCommonRequestBroker}';
            GS.gainsightTabs = fakeEval('{!JSINHTMLENCODE(GainsightTabs)}');

            GS.commonRemotingEndPoint   = '{!$RemoteAction.GSCommonBroker.gsCommonRequestBroker}';
            GS.remotingCommonBulkEndPoint  ='{!$RemoteAction.GSCommonBroker.gsCommonBulkRequestBroker}';
    		GS.userConfig = {
                                userLocale          : {!(dateTimeFormatByLocale)},
                                userName            : "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}",
                                userId              : "{!$User.Id}",
                                emailId             : "{!$User.Email}",
                                profilePhoto        : "{!(profilePhoto)}",
                                userDisplayName     : "{!(JSINHTMLENCODE(userDisplayName))}",
                                gainsightTabs       : fakeEval('{!JSINHTMLENCODE(gainsightTabs)}'),
                                isLightningEnabled  : {!(isLightningEnabled)}
                            };      
        	GS.currencyConfig = fakeEval('{!JSINHTMLENCODE(currencyConfigJSON)}');
            var host = "SALESFORCE";
    		GS.appName = "gainsight-ng-ui/objects/company-admin";

            var urlParams =  window.location.search
                .substring(1)
                .split("&")
                .map(function(v) {
                    return v.split("=");
                })
                .reduce(function(acc, arr) {
                    if (acc[arr[0]]) {
                        if (!Array.isArray(acc[arr[0]])) {
                            acc[arr[0]] = [acc[arr[0]]]
                        }
                        if(arr[0]) {
                            acc[arr[0]].push(decodeURIComponent(arr[1]))
                        }
                    } else {
                        if(arr[0]) {
                            acc[arr[0]] = decodeURIComponent(arr[1]);
                        }
                    }
                    return acc;
                }, {});

            
        </script>
